{
  "id": null,
  "uid": "genai-overview",
  "title": "GenAI Observability Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "tags": [
    "otel",
    "genai"
  ],
  "panels": [
    {
      "type": "timeseries",
      "title": "GenAI Operation Latency",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.50, sum by (le, gen_ai_request_model) (rate(gen_ai_client_operation_duration_seconds_bucket[5m])))",
          "legendFormat": "p50 {{gen_ai_request_model}}"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.95, sum by (le, gen_ai_request_model) (rate(gen_ai_client_operation_duration_seconds_bucket[5m])))",
          "legendFormat": "p95 {{gen_ai_request_model}}"
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.99, sum by (le, gen_ai_request_model) (rate(gen_ai_client_operation_duration_seconds_bucket[5m])))",
          "legendFormat": "p99 {{gen_ai_request_model}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "id": 1
    },
    {
      "type": "timeseries",
      "title": "Token Utilization",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (gen_ai_token_type, gen_ai_request_model) (rate(gen_ai_client_token_usage_sum[5m]))",
          "legendFormat": "{{gen_ai_token_type}} {{gen_ai_request_model}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "tokens/s"
        }
      },
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "id": 2
    },
    {
      "type": "timeseries",
      "title": "Error Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (gen_ai_request_model) (rate(gen_ai_client_operation_errors_total[5m]))",
          "legendFormat": "errors {{gen_ai_request_model}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 8,
        "h": 8
      },
      "id": 3
    },
    {
      "type": "timeseries",
      "title": "Cost per Minute",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (gen_ai_request_model) (rate(gen_ai_client_operation_cost_usd_sum[5m]))",
          "legendFormat": "cost {{gen_ai_request_model}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "usd"
        }
      },
      "gridPos": {
        "x": 8,
        "y": 8,
        "w": 8,
        "h": 8
      },
      "id": 4
    },
    {
      "type": "timeseries",
      "title": "Tool Call Breakdown",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (tool_name, gen_ai_request_model) (rate(gen_ai_client_tool_calls_total[5m]))",
          "legendFormat": "{{tool_name}} ({{gen_ai_request_model}})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "calls/s"
        }
      },
      "gridPos": {
        "x": 16,
        "y": 8,
        "w": 8,
        "h": 8
      },
      "id": 5
    },
    {
      "type": "timeseries",
      "title": "RAG Retrieval Latency",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, datasource) (rate(gen_ai_rag_retrieval_latency_milliseconds_bucket[5m]))) / 1000",
          "legendFormat": "p95 {{datasource}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "id": 6
    },
    {
      "type": "traceql-search",
      "title": "Recent Traces",
      "datasource": {
        "type": "tempo",
        "uid": "tempo"
      },
      "targets": [
        {
          "refId": "A",
          "query": "{ service.name = \"genai-rag-service\" } | limit 20"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "id": 7
    }
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-30m",
    "to": "now"
  }
}
